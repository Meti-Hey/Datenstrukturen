public class Node<E> {

    private Node nachfolger;
    private E zeigeAuf;

    public Node(E pZeigeAuf) {
        zeigeAuf = pZeigeAuf;
        nachfolger = null;
    }

    public Node getNachfolger() {
        return nachfolger;
    }

    public void setNachfolger(Node nachfolger) {
        this.nachfolger = nachfolger;
    }

    public void setZeigeAuf(E zeigeAuf) {
        this.zeigeAuf = zeigeAuf;
    }

    public E getZeigeAuf() {
        return zeigeAuf;
    }
}
